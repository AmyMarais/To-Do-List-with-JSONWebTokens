  "dependencies": {
    "bcryptjs": "2.4.3",
    "concurrently": "6.2.0",
    "config": "3.3.6",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.12.13"
  },
  "devDependencies": {
    "nodemon": "2.0.7"
  }
}
